package GunLab
import OperatableUnit
import TlsItemIds
import Camp

public class GunLab extends OperatableUnit

  construct (ICamp camp, unit u)
    super(camp, u)

  override protected function onOperated()
    AddItemToStock(this._unit, TlsItemIds.huntingRifle, 1, 1)

  override protected function onUnoperated()
    RemoveItemFromStock(this._unit, TlsItemIds.huntingRifle)