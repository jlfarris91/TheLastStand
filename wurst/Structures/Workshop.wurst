package Workshop
import TlsItemIds
import OperatableUnit
import Camp

public class Workshop extends OperatableUnit

  construct (ICamp camp, unit u)
    super(camp, u)

  override protected function onOperated()
    AddItemToStock(this._unit, TlsItemIds.basicTools, 1, 1)

  override protected function onUnoperated()
    RemoveItemFromStock(this._unit, TlsItemIds.basicTools)