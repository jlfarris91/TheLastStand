package OnlyOnceWaveActivator
import WaveActivator
import Runnable

// ============================================================================
public class OnlyOnceWaveActivator extends WaveActivatorBase

  // --------------------------------------------------------------------------
  override function onStarted()
    super.onStarted()
    raiseActivatedEvent()

  // --------------------------------------------------------------------------
  override function getState() returns RunnableState
    return super.getState()

  // --------------------------------------------------------------------------
  override function getError() returns string
    return super.getError()

  // --------------------------------------------------------------------------
  override function start()
    super.start()

  // --------------------------------------------------------------------------
  override function pause()
    super.pause()

  // --------------------------------------------------------------------------
  override function resume()
    super.resume()

  // --------------------------------------------------------------------------
  override function complete()
    super.complete()

  // --------------------------------------------------------------------------
  override function fail(string errorMsg)
    super.fail(errorMsg)