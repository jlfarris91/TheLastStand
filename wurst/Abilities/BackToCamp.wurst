package BackToCamp
import RegisterEvents
import Camp

constant int ABILITY_ID_BACKTOCAMP = 'A00J'

function onBackToCampAbilityCast()
  var u = GetSpellAbilityUnit()
  var p = u.getOwner()
  var camp = p.getCamp()
  var campUnit = camp.getCampUnit()
  SelectUnitForPlayerSingle(campUnit, p)
  u.issuePointOrder("move", campUnit.getPos())

init 
  registerSpellEffectEvent(ABILITY_ID_BACKTOCAMP, function onBackToCampAbilityCast)