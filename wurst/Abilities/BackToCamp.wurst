package BackToCamp
import RegisterEvents
import Camp
import HumanPlayerMetadata
import Orders
import TlsAbilityIds

function onBackToCampAbilityCast()
  var u = GetSpellAbilityUnit()
  var p = u.getOwner()
  var camp = p.getHumanMetadataRequired().getCamp()
  var campUnit = camp.getCampUnit()
  u.issuePointOrderById(OrderIds.move, campUnit.getPos())

init 
  registerSpellEffectEvent(TlsAbilityIds.backToCamp, function onBackToCampAbilityCast)