package Printing_config
import initlater Wurst
import initlater Console
// import initlater Console

@config public var DEBUG_LEVEL = Loglevel.TRACE

@config public function printLog(player _showTo, Loglevel loglvl, string msg)
  if (not Console.getIsReady())
    return
  if (DEBUG_LEVEL castTo int) <= (loglvl castTo int)
    let compositeMsg = loglvl.getTag() + " - " + msg
    Console.addText(compositeMsg)
