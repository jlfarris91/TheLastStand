package CampUnit
import initlater ICampUnit
import initlater ICamp
import SmartUnit

// ============================================================================
public class CampUnit extends SmartUnit implements ICampUnit
  protected ICamp _camp

  // --------------------------------------------------------------------------
  construct(ICamp camp, unit u)
    super(u)
    this._camp = camp

  // --------------------------------------------------------------------------
  ondestroy
    this._camp = null

  // --------------------------------------------------------------------------
  @inline
  override function getCamp() returns ICamp
    return this._camp
    
  // --------------------------------------------------------------------------
  @inline
  override function getUnit() returns unit
    return super.getUnit()