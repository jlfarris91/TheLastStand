package CampUnit
import ICampUnit
import ICamp

public class CampUnit implements ICampUnit
  protected unit _unit
  protected ICamp _camp

  construct(ICamp camp, unit u)
    this._camp = camp
    this._unit = u

  ondestroy
    this._camp = null
    if (this._unit != null)
      this._unit.remove()
    this._unit = null

  override function getUnit() returns unit
    return this._unit

  override function getCamp() returns ICamp
    return this._camp