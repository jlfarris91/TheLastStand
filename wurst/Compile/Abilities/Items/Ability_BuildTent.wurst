package Ability_BuildTent
import AbilityObjEditing
import TlsAbilityIds
import TlsUnitIds
import ObjectIds

// ============================================================================
@compiletime function createAbility()
  new AbilityDefinitionBuildTinyScoutTower(TlsAbilityIds.buildTent)
    ..setDurationNormal(1, 1.0)
    ..setUnitCreatedperplayerrace(1, commaList(TlsUnitIds.tent))
    ..setIconNormal("ReplaceableTextures\\CommandButtons\\BTNTent.blp")