package Ability_BuildTurret
import AbilityObjEditing
import TlsAbilityIds
import TlsUnitIds
import ObjectIds

// ============================================================================
@compiletime function createAbility()
  new AbilityDefinitionBuildTinyScoutTower(TlsAbilityIds.buildPortableTurret)
    ..setDurationNormal(1, 1.0)
    ..setUnitCreatedperplayerrace(1, commaList(TlsUnitIds.turretPortable))
    ..setIconNormal("ReplaceableTextures\\CommandButtons\\BTNTent.blp")