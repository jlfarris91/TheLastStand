package Ability_SurvivorInventory
import AbilityObjEditing
import TlsAbilityIds

@compiletime function createInventory1Slot()
  new AbilityDefinitionInventory(TlsAbilityIds.survivorInventory)
    ..setLevels(5)
    ..setItemCapacity(1, 2)
    ..setItemCapacity(2, 3)
    ..setItemCapacity(3, 4)
    ..setItemCapacity(4, 5)
    ..setItemCapacity(5, 6)
    ..setEditorSuffix("(Survivor)")
    ..setRace(Race.Human)