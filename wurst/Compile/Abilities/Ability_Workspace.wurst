package Ability_Workspace
import AbilityObjEditing
import TlsAbilityIds
import ObjectIds
import TlsUnitIds

// ============================================================================
@compiletime function createAbility()
  new AbilityDefinitionCargoHoldTransport(TlsAbilityIds.workspaceCargoHold)..setCargoCapacity(1, 1)
  new AbilityDefinitionLoad(TlsAbilityIds.workspaceLoad)..setAllowedUnitType(1, commaList(TlsUnitIds.Survivors.survivorMale))