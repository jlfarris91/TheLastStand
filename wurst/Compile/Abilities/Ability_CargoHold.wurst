package Ability_CargoHold
import AbilityObjEditing
import TlsAbilityIds

// ============================================================================
@compiletime function createAbility()
  new AbilityDefinitionCargoHoldTransport(TlsAbilityIds.shelter1CargoHold)..setCargoCapacity(1, 1)
  new AbilityDefinitionCargoHoldTransport(TlsAbilityIds.shelter2CargoHold)..setCargoCapacity(1, 2)
  new AbilityDefinitionCargoHoldTransport(TlsAbilityIds.shelter3CargoHold)..setCargoCapacity(1, 4)
