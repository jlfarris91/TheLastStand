package Unit_DarkMinions
import TlsUnitDefinition

// ============================================================================
@compiletime function createDarkMinions()

  new TlsUnitDefinition(TlsUnitIds.Undead.lesserDarkMinion, UnitIds.darkminion1)
  new TlsUnitDefinition(TlsUnitIds.Undead.darkMinion, UnitIds.darkminion2)
  new TlsUnitDefinition(TlsUnitIds.Undead.greaterDarkMinion, UnitIds.darkminion3)